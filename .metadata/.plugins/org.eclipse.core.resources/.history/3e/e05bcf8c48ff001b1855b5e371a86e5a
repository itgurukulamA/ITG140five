import java.util.Scanner;

public class Palindrome {

	public static void main(String[] args) {
		String a,b="";
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the String you want to check");
		a=s.nextLine();
		int n = a.length();
		for(int i=n-1;i>=0;i--) {
			b=b+a.charAt(i);
			
		}
		
	}

}
