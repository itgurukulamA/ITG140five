package Exaception;

class MyException extends Exception
{
 public MyException(String s)
 {
     super(s);
 }
}
public class CoustomEx
{
 public static void main(String args[])
 {
     try
     {
         throw new MyException("laxman");
     }
     catch (MyException ex)
     {
         System.out.println(ex.getMessage());
     }
 }
}
