package Collections;
import java.util.*;
import java.util.Map.Entry;
public class Demo10 {
		public static void main(String args[])
		{
			Map m=new HashMap();
			m.put(1,"laxman");
			m.put(2, "suresh");
			m.put(3,"rakesh");
			Set s=m.entrySet();
			Iterator itr=s.iterator();
			while(itr.hasNext())
			{
				Map.Entry em=(Entry) itr.next();
				System.out.println(em.getKey()+" "+em.getValue());
			}
		}
}
