package Collections;
import java.util.*;
public class Demo1 {
public static void main(String args[])
{
	List n=new ArrayList();
	n.add(12);
	n.add("laxman");
	n.add(456.38);
	n.add("Suresh");
	n.add('r');
	n.add(12);
	n.add("laxman");
	System.out.println(n);
	System.out.println(n.remove(2));
	System.out.println(n);
	System.out.println(n.size());
}
}
