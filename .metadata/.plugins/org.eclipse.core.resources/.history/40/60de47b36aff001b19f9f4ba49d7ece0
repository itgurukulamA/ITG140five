package StringEx;
import java.util.Scanner;
public class CommonWordsEx {

	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	String a="";
	String b="";
	String c="";
	String ch1="";
	System.out.println("Enter two Strings");
	a=sc.nextLine();
	b=sc.nextLine();
	int d=a.length();
	for(int i=0;i<d;i++)
	{
		char ch=a.charAt(i);
		ch1=""+ch;
	}
	}

}
