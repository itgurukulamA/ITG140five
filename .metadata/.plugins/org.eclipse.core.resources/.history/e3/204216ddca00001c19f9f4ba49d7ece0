package Collections;
import java.util.*;
public class Demo5 {
		public static void main(String args[])
		{
			List al=new ArrayList();
			al.add(1);
			al.add(2);
			al.add(3);
			al.add(4);
			al.add(7);
			System.out.println(al);
			Iterator itr=al.iterator();
			while(itr.hasNext())
			{
				System.out.println(itr.next());
			}
		}
}
